  "repository": "github:ccarcaci/tokenator",
  "main": "src/index.js",
  "devDependencies": {
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "stream-mock": "^2.0.5",
    "minimist": ">=1.2.2"
